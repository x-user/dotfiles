# ~/.gitconfig_aliases
# ==============================================================================

[alias]
  br = branch
  ci = commit
  co = checkout
  st = status

  adog = log --all --decorate --oneline --graph
  oops = commit --amend --no-edit

  alias = !git config --get-regexp '^alias\\.' | sed -e 's/^alias\\.//' -e 's/\\ /=/'

# ==============================================================================
# code: language=properties vim: set ft=gitconfig:
